{
  "name": "gss-ui",
  "version": "0.0.12",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/gss-ui.umd.cjs",
  "module": "./dist/gss-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/gss-ui.js",
      "require": "./dist/gss-ui.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1",
    "@react-types/shared": "^3.22.1",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-interactions": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/addon-onboarding": "^8.0.10",
    "@storybook/blocks": "^8.0.10",
    "@storybook/react": "^8.0.10",
    "@storybook/react-vite": "^8.0.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.0.10",
    "tailwindcss-react-aria-components": "^1.0.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.8.3",
    "vite-plugin-remove-console": "^2.2.0",
    "yarn": "^1.22.22"
  },
  "scripts": {
    "format": "prettier --write \"./src/**/*.ts{,x}\"",
    "prebuild": "yarn format",
    "build": "tsc && vite build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "preview": "npx serve ./storybook-static",
    "prepreview": "yarn build-storybook"
  },
  "dependencies": {
    "@storybook/test": "^8.0.10",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "lucide-react": "^0.292.0",
    "postcss": "^8.4.31",
    "react-aria": "^3.32.1",
    "react-aria-components": "^1.0.0-rc.0",
    "react-stately": "^3.30.1",
    "tailwind-variants": "^0.1.18",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "prettier": {
    "tailwindConfig": "./tailwind.config.js",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
